{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://sunny-recreation.up.railway.app/api/v1",
    "VITE_WS_URL": "wss://sunny-recreation.up.railway.app",
    "VITE_APP_NAME": "Email Productivity Agent"
  }
}
